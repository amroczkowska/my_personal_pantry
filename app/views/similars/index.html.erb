<div class="page-header">
  <h1>
    Similars
    <a href="/similars/new" class="btn btn-lg btn-success">New Similar</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Similar name</th>
        <th>Foods</th>
        <th>Actions</th>
      </tr>

      <% @similars.each do |similar| %>
      <tr>
        <td><%= similar.similar_name %></td>
        <td><%= similar.foods_id %></td>
        <td>
          <a href="/similars/<%= similar.id %>" class="btn btn-primary">Show</a>
          <a href="/similars/<%= similar.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_similar/<%= similar.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
