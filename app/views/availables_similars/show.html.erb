<div class="page-header">
  <h3>Availables Similar #<%= @availables_similar.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Similar</dt>
      <dd>
        <% if @availables_similar.similar_foods.present? %>
          <a href="/similars/<%= @availables_similar.similar_id %>">
            <%= @availables_similar.similar_foods.similar_name %>
          </a>
        <% end %>
      </dd>

      <dt>Availables</dt>
      <dd>
        <% if @availables_similar.availables.present? %>
          <a href="/availables/<%= @availables_similar.availables_id %>">
            <%= @availables_similar.availables.foods_id %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/availables_similars" class="btn btn-primary">
        Back
      </a>
      <a href="/availables_similars/<%= @availables_similar.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_availables_similar/<%= @availables_similar.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
